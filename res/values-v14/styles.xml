<?xml version="1.0" encoding="utf-8"?>
<resources>
    <style name="TextAppearance.StatusBar.EventContent" parent="@android:style/TextAppearance.StatusBar.EventContent" />
    <style name="TextAppearance.StatusBar.EventContent.Title" parent="@android:style/TextAppearance.StatusBar.EventContent.Title" />
    <style name="TextAppearance.StatusBar.EventContent.Line2" parent="@style/TextAppearance.StatusBar.EventContent">
        <item name="android:textSize">@dimen/notification_subtext_size</item>
    </style>
    <style name="TextAppearance.StatusBar.EventContent.Info" parent="@style/TextAppearance.StatusBar.EventContent" />
    <style name="TextAppearance.StatusBar.EventContent.Time" parent="@style/TextAppearance.StatusBar.EventContent" />
    <style name="Base.TextAppearance.AppCompat.Button" parent="@style/Base.TextAppearance.AppCompat">
        <item name="android:textSize">@dimen/abc_text_size_button_material</item>
        <item name="android:textColor">?android:textColorPrimary</item>
        <item name="android:textAllCaps">true</item>
    </style>
    <style name="Platform.AppCompat" parent="@style/Platform.V14.AppCompat" />
    <style name="Platform.AppCompat.Light" parent="@style/Platform.V14.AppCompat.Light" />
    <style name="Platform.V14.AppCompat" parent="@style/Platform.V11.AppCompat">
        <item name="android:actionModeSelectAllDrawable">?actionModeSelectAllDrawable</item>
        <item name="android:listPreferredItemPaddingLeft">@dimen/abc_list_item_padding_horizontal_material</item>
        <item name="android:listPreferredItemPaddingRight">@dimen/abc_list_item_padding_horizontal_material</item>
    </style>
    <style name="Platform.V14.AppCompat.Light" parent="@style/Platform.V11.AppCompat.Light">
        <item name="android:actionModeSelectAllDrawable">?actionModeSelectAllDrawable</item>
        <item name="android:listPreferredItemPaddingLeft">@dimen/abc_list_item_padding_horizontal_material</item>
        <item name="android:listPreferredItemPaddingRight">@dimen/abc_list_item_padding_horizontal_material</item>
    </style>
    <style name="MaterialAlertDialogTitle">
        <item name="android:textAppearance">@style/TextAppearance.AppCompat.Title</item>
        <item name="android:textColor">#ff000000</item>
    </style>
    <style name="Material.AlertDialog" parent="@style/Theme.AppCompat.Light.Dialog.Alert">
        <item name="android:textColorPrimary">#ff000000</item>
        <item name="android:windowTitleStyle">@style/MaterialAlertDialogTitle</item>
        <item name="android:background">#ffffffff</item>
        <item name="android:windowMinWidthMajor">@android:dimen/dialog_min_width_major</item>
        <item name="android:windowMinWidthMinor">@android:dimen/dialog_min_width_minor</item>
        <item name="colorControlHighlight">#ffd4d4d4</item>
    </style>
    <style name="Material.ProgressDialog" parent="@style/Theme.AppCompat.Light.Dialog">
        <item name="android:windowBackground">@android:color/transparent</item>
        <item name="android:windowMinWidthMajor">@android:dimen/dialog_min_width_major</item>
        <item name="android:windowMinWidthMinor">@android:dimen/dialog_min_width_minor</item>
    </style>
    <style name="Material.EditText" parent="@android:style/Widget.EditText">
        <item name="android:background">@null</item>
        <item name="android:paddingLeft">8.0dip</item>
        <item name="android:paddingRight">8.0dip</item>
    </style>
    <style name="Material.ListView" parent="@android:style/Widget.ListView">
        <item name="android:dividerHeight">1.0px</item>
    </style>
    <style name="Material.DrawerArrow" parent="@style/Widget.AppCompat.DrawerArrowToggle">
        <item name="color">#ff0a4d8c</item>
        <item name="spinBars">true</item>
    </style>
    <style name="Theme.Material" parent="@style/Theme.AppCompat">
        <item name="android:textColorPrimary">#ff0a4d8c</item>
        <item name="android:windowBackground">@android:color/white</item>
        <item name="android:windowContentOverlay">@drawable/actionbar_shadow</item>
        <item name="android:editTextStyle">@style/Material.EditText</item>
        <item name="android:listViewStyle">@style/Material.ListView</item>
        <item name="android:textColorHint">#ffc7c7cd</item>
        <item name="android:dialogTheme">@style/Material.ProgressDialog</item>
        <item name="android:alertDialogTheme">@style/Material.ProgressDialog</item>
        <item name="android:textCursorDrawable">@null</item>
        <item name="android:textAllCaps">false</item>
        <item name="dialogTheme">@style/Material.AlertDialog</item>
        <item name="colorControlNormal">#ff0a4d8c</item>
        <item name="colorControlHighlight">#ffd4d4d4</item>
        <item name="alertDialogTheme">@style/Material.AlertDialog</item>
        <item name="drawerArrowStyle">@style/Material.DrawerArrow</item>
    </style>
    <style name="Theme.Material.NoActionBar" parent="@style/Theme.Material">
        <item name="android:windowContentOverlay">@null</item>
        <item name="windowActionBar">false</item>
        <item name="windowNoTitle">true</item>
    </style>
    <style name="Theme.Material.Translucent.NoActionBar" parent="@style/Theme.Material.NoActionBar">
        <item name="android:windowBackground">@android:color/transparent</item>
        <item name="android:windowIsTranslucent">true</item>
        <item name="android:colorBackgroundCacheHint">@null</item>
    </style>
    <style name="Theme.Material.Splash" parent="@style/Theme.Material.NoActionBar">
        <item name="android:windowBackground">@drawable/background</item>
    </style>
</resources>
